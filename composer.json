{
    "name": "sankokai/press",
    "description": "Un élégant Markdown-powered blog pour le framework Laravel",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sankokai\\Press\\": "src/",
            "Sankokai\\Press\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sankokai\\Press\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Matthieu ILY",
            "email": "matthieu.ily@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^7.15",
        "laravel/legacy-factories": "^1.3"
    },
    "require": {
        "php": "8.1.*",
        "erusev/parsedown": "^1.7"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sankokai\\Press\\PressBaseServiceProvider"
            ] 
        }
    }
}
